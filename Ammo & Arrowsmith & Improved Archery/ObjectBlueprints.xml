<!-- // Incendiary Bullets, uses XRL.World.Parts, TemperatureOnHit -->
  <object Name="Incendiary Bullet" Inherits="Projectile">
    <part Name="Render" DisplayName="&amp;oincendiary &amp;ybullet" ColorString="&amp;o" />
    <part Name="AmmoSlug" ProjectileObject="ProjectileIncendiaryBullet" />
    <part Name="Physics" Category="Ammo" />
    <part Name="Description" Short="An odd shell casing with a strange orange tip, it emanates a hot aura, and bursts in a blast of heat on impact." />
    <part Name="TinkerItem" Bits="BC" CanBuild="true" CanRepair="false" CanDisassemble="false" NumberMade="6" Ingredient="Vinewafer Sheaf"/>
    <part Name="Commerce" Value="0.20" />
    <tag Name="AlwaysStack" Value="Yes" />
    <tag Name="DynamicObjectsTable:Ammo" />
  </object>
  <object Name="ProjectileIncendiaryBullet" Inherits="TemporaryProjectile">
    <part Name="Projectile" BasePenetration="0" StrengthPenetration="1" BaseDamage="1d2" ColorString="&amp;o" />
    <part Name="Physics" IsReal="false" />
    <part Name="TemperatureOnHit" Amount="400" Max="false" OnWielderHit="true" />
  </object>


